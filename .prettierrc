{
	"trailingComma": "none",
	"useTabs": true,
	"semi": true,
	"printWidth": 80,
	"arrowParens": "always",
	"importOrderSeparation": true,
	"importOrder": [
		"@rollup/.*",
		"rollup-.*",
		"^..?/.*",
		"^use-local-storage-state$",
		"^styled-components$",
		"^react.*",
		"^preact$",
		"^preact.*",
		"^axios.*",
		"^@src/.*",
		"^@models/.*",
		"^@interfaces/.*",
		"^@styles/.*",
		"^@shared/.*",
		"^@utils/.*",
		"^@components/.*",
		"^@svg/.*"
	]
}
