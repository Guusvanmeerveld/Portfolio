{
	"trailingComma": "none",
	"useTabs": true,
	"semi": true,
	"printWidth": 80,
	"arrowParens": "always",
	"importOrderSeparation": true,
	"importOrder": [
		"^..?/.*",
		"^react.*",
		"^axios.*",
		"@prisma/.*",
		"^@src/.*",
		"^@models/.*",
		"^@interfaces/.*",
		"^@styles/.*",
		"^@shared/.*",
		"^@utils/.*",
		"^@components/.*",
		"^@cache/.*",
		".*scss$",
		".*css$",
		"^@svg/.*"
	],
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
